{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "/auth/$1"
    },
    {
      "src": "/user/(.*)",
      "dest": "/user/$1"
    },
    {
      "src": "/news/(.*)",
      "dest": "/news/$1"
    },
    {
      "src": ".*",
      "dest": "./index.js"
    }
  ]
}